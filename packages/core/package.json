  "license": "ISC",
  "homepage": "https://github.com/TaiAiAc/quiteer-vue-directives#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TaiAiAc/quiteer-vue-directives.git"
  },
  "bugs": {
    "url": "https://github.com/TaiAiAc/quiteer-vue-directives/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": ["vue3"],
