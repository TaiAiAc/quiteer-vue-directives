  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "release": "npm publish"
  },
  "peerDependencies": {
    "vue": ">= 3"
  },
  "dependencies": {
    "vue": "^3.2.47"
  },
  "devDependencies": {}
